{
  "family": "gallery-app-task-definition",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::677276091734:role/gallery-app-ECSTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::677276091734:role/gallery-app-ecs-task-role",
  "containerDefinitions": [
    {
      "name": "gallery-app-container",
      "image": "677276091734.dkr.ecr.eu-west-1.amazonaws.com/gallery-app-repository:latest",
      "portMappings": [
        {
          "containerPort": 8080
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/gallery-app-log-group",
          "awslogs-region": "eu-west-1",
          "awslogs-stream-prefix": "gallery-app"
        }
      }
    }
  ]
}
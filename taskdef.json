{
  "family": "gallery-app-task-definition",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::677276091734:role/gallery-app-stack-ECSTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "gallery-app-stack-container",
      "image": "<IMAGE1_NAME>",
      "portMappings": [
        {
          "containerPort": 8080
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/gallery-app-log-group",
          "awslogs-region": "eu-west-1",
          "awslogs-stream-prefix": "gallery-app-container-log"
        }
      },
      "HealthCheck": {
        "Command": [
          "CMD-SHELL",
          "curl -f http://localhost:8080/ || exit 1"
        ],
        "Interval": 30,
        "Timeout": 5,
        "Retries": 3,
        "StartPeriod": 120
      }
    }
  ]
}